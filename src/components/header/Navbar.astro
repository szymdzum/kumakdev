---
import Brand from "./Brand.astro";
import Menu from "./Menu.astro";
import Header from "./Header.astro";
import SocialLinks from "./SocialLinks.astro";

const brandName = "PuttoPack";
const menu = [
  {
    url: "/",
    text: "Home",
  },
  {
    url: "/about",
    text: "About",
  },
  {
    url: "/blog",
    text: "Blog",
  },
  {
    url: "/contact",
    text: "Contact",
  },
];
const instaUrl = "https://www.instagram.com/puttopack";
---

<Header>
  <Brand name={brandName} />
  <Menu menu={menu} />
  <SocialLinks instaUrl={instaUrl} />
</Header>
